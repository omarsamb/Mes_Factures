<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.mesfactures.ui.presentation.PresentationFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_presentation" />

    <fragment
        android:id="@+id/navigation_invoice"
        android:name="com.example.mesfactures.ui.invoice.InvoiceFragment"
        android:label="@string/title_invoice"
        tools:layout="@layout/fragment_invoice" />

    <fragment
        android:id="@+id/navigation_help"
        android:name="com.example.mesfactures.ui.help.HelpFragment"
        android:label="@string/title_help"
        tools:layout="@layout/fragment_help" />
</navigation>